@page
@model WebApplication10.Pages.Presets.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Удаление</h1>

<h3>Вы уверенны что хотите удалить данный элемент?</h3>
<div>
    <h4>Preset</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Preset.Healthplayer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Preset.Healthplayer.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Preset.Speedplayer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Preset.Speedplayer.SpeedplayerID)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Preset.PresetID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-page="./Index">Вернуться к списку</a>
    </form>
</div>
