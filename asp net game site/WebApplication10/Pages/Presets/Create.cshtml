@page
@model WebApplication10.Pages.Presets.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Создание</h1>

<h4>Preset</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Preset.HealthplayerID" class="control-label"></label>
                <select asp-for="Preset.HealthplayerID" class ="form-control" asp-items="ViewBag.HealthplayerID"></select>
            </div>
            <div class="form-group">
                <label asp-for="Preset.SpeedplayerID" class="control-label"></label>
                <select asp-for="Preset.SpeedplayerID" class ="form-control" asp-items="ViewBag.SpeedplayerID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Создать" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Вернуться к списку</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
